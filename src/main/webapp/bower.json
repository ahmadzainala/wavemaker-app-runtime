{
    "name": "wavemaker-lib",
    "version": "8.0",
    "authors": [],
    "dependencies": {
        "jquery": "2.1.4",
        "jquery-ui": "1.11.4",
        "angular": "1.4.8",
        "angular-animate": "1.4.8",
        "bootstrap": "3.3.5",
        "font-awesome": "4.4.0",
        "angular-sanitize": "1.4.8",
        "angular-bootstrap-colorpicker": "https://github.com/buberdds/angular-bootstrap-colorpicker.git#v3.0.18",
        "angular-route": "1.4.8",
        "angular-scenario": "1.4.8",
        "angular-ui-bootstrap": "https://github.com/angular-ui/bootstrap-bower.git#v1.1.0",
        "angular-toaster": "https://github.com/jirikavi/AngularJS-Toaster.git#0.4.15",
        "textAngular": "https://github.com/fraywing/textAngular.git#v1.4.2",
        "d3": "https://github.com/mbostock/d3.git#v3.5.6",
        "nvd3": "https://github.com/novus/nvd3.git#v1.8.1",
        "placeholders": "4.0.1",
        "socialbyway": "https://github.com/Imaginea/SocialByWay.git",
        "xdomain": "0.7.3",
        "ocLazyLoad": "https://github.com/ocombe/ocLazyLoad.git#1.0.4",
        "lodash": "https://github.com/lodash/lodash.git#4.0.0",
        "hammerjs": "2.0.4",
        "animate.css": "4.0.0.alpha-1",
        "fullcalendar": "2.3.2",
        "angular-ui-calendar": "1.0.0",
        "moment": "2.10.6",
        "ngCordova":"v0.1.20-alpha"
    },
    "resolutions": {
        "d3": "v3.5.6",
        "angular": "1.4.8",
        "fullcalendar": "2.3.2"
    },
    "exportsOverride": {
        "jquery-ui": {
            "themes": "themes/base/*.css",
            "themes/images": "themes/base/images/*.*",
            "js": "jquery-ui.js"
        },
        "animate.css": {
            ".": ["dist/*.css"]
        },
        "bootstrap": {
            "less": "less/",
            "css": [
                "dist/css/*.css",
                "dist/css/*.map"
            ],
            "fonts": "fonts/"
        },
        "font-awesome": {
            "css": "css/*.css",
            "fonts": "fonts/*"
        },
        "placeholders": {
            "js": "lib/*.js"
        },
        "socialbyway": {
            "js": [
                "dist/js/socialbyway.v1.01-beta.js",
                "dist/js/socialbyway.ui.v1.01-beta.js"
            ],
            "css": "dist/style/default/socialbyway.ui.v1.01-beta.css",
            "css/images": "dist/style/default/images/*.*"
        },
        "textAngular": {
            "js": [
                "dist/textAngular.min.js",
                "dist/textAngular-rangy.min.js"
            ]
        },
        "ocLazyLoad": {
            ".": [
                "dist/ocLazyLoad.min.js"
            ]
        }
    }
}
