    "jquery": "2.1.3",
    "jquery-ui": "1.11.2",
    "angular": "1.4.0",
    "angular-animate": "1.4.0",
    "bootstrap": "3.3.2",
    "angular-sanitize": "1.4.0",
    "angular-bootstrap-colorpicker": "https://github.com/buberdds/angular-bootstrap-colorpicker.git#v3.0.11",
    "angular-route": "1.4.0",
    "angular-scenario": "1.4.0",
    "angular-ui-bootstrap": "https://github.com/angular-ui/bootstrap-bower.git#v0.12.0",
    "angular-toaster": "https://github.com/jirikavi/AngularJS-Toaster.git#0.4.13",
    "textAngular": "https://github.com/fraywing/textAngular.git#v1.3.7",
    "d3": "https://github.com/mbostock/d3.git#v3.5.5",
    "nvd3": "https://github.com/novus/nvd3.git#v1.8.1-alpha",
    "placeholders": "3.0.2",
    "socialbyway": "https://github.com/Imaginea/SocialByWay.git",
    "xdomain": "0.6.17",
    "ocLazyLoad": "https://github.com/ocombe/ocLazyLoad.git#0.5.0",
    "underscore": "https://github.com/jashkenas/underscore.git#1.7.0",
    "hammerjs": "2.0.4",
    "animate.css": "4.0.0.alpha-1",
    "angular-ui-calendar": "0.9.0-beta.1",
    "fullcalendar" : "2.3.1",
    "moment" : "2.10.2"
  },
  "resolutions": {
    "d3": "v3.5.5",
    "angular": "1.4.0"
  },
  "exportsOverride": {
    "jquery-ui": {
      "themes": "themes/base/*.css",
      "themes/images": "themes/base/images/*.*",
      "js": "jquery-ui.js"
    },
    "animate.css": {
      "." : ["dist/*.css"]
    },
    "bootstrap" : {
      "less": "less/",
      "css": ["dist/css/*.css", "dist/css/*.map"],
      "fonts": "fonts/"
    },
    "placeholders": {
      "js": "lib/*.js"
    },
    "socialbyway": {
      "js": ["dist/js/socialbyway.v1.01-beta.js", "dist/js/socialbyway.ui.v1.01-beta.js"],
      "css": "dist/style/default/socialbyway.ui.v1.01-beta.css",
      "css/images": "dist/style/default/images/*.*"
    },
    "textAngular": {
      "js": ["dist/textAngular.min.js","dist/textAngular-rangy.min.js" ]
    }
  }
}
